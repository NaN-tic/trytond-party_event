<tryton>
    <data>
        <record model="ir.ui.view" id="party_view_form">
            <field name="model">party.party</field>
            <field name="inherit" ref="party.party_view_form"/>
            <field name="arch" type="xml">
                <![CDATA[
                <data>
                    <xpath
                        expr="/form/notebook"
                        position="inside">
                        <page string="Events" id="event">
                            <field name="events"/>
                        </page>
                    </xpath>
                </data>
                ]]>
            </field>
        </record>
    </data>
</tryton>
